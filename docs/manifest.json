{
  "manifest_version": 3,
  "name": "웨,일해(Whale Hae)",
  "description": "웨일대학 1팀 - 일정 관리 및 링크 자동 실행",
  "version": "0.1.1",
  "sidebar_action": {
    "default_page": "index.html",
    "default_icon": {
      "16": "logo.png",
      "32": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    },
    "default_title": "웨,일해",
    "use_navigation_bar": false
  },
  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "permissions": [
    "unlimitedStorage",
    "storage",
    "tabs",
    "notifications",
    "alarms"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
